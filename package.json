{
  "name": "study-for-net-super-front",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "start": "nuxt-ts start",
    "generate": "nuxt-ts generate",
    "test-all": "jest",
    "test": "jest --watch",
    "mock": "json-server --watch ./mock/db.json --routes ./mock/routes.json --port 3001"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.5",
    "core-js": "^3.6.5",
    "nuxt": "^2.14.6",
    "vuejs-paginate": "^2.1.0"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.11",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxtjs/tailwindcss": "^3.3.4",
    "@types/jest": "^26.0.19",
    "@vue/test-utils": "^1.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.5.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-vue": "^7.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.5.0",
    "json-server": "^0.16.3",
    "prettier": "^2.2.1",
    "prettier-plugin-tailwind": "^2.2.8",
    "ts-jest": "^26.4.1",
    "vue-jest": "^3.0.4"
  },
  "jest": {
    "setupFiles": [
      "./test/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1",
      "^~/(.*)$": "<rootDir>/$1",
      "^vue$": "vue/dist/vue.common.js",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock/fileMock.js"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "bower_components",
      "shared"
    ],
    "transform": {
      "\\.js$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/test/mock/fileTransformer.js"
    }
  }
}
